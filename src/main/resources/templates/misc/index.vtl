<section class="index_header equipment-header">
    <h1>Misc</h1>
</section>
<div class="all_items">
    #foreach ($m in $misc)
    <div class="single_item" style="background-image: url($m.getImage())">
        <p>$m.getName()</p>
        <p>$m.prettyPrice()</p>
        <a href="/misc/$m.getId()">Show More</a>
    </div>
    #end
</div>