<section class="index_header equipment-header">
    <h1>Misc</h1>
</section>
<div class="all_items">
    #foreach ($m in $misc)
    <a href="/misc/$m.getId()">
    <div class="single_item" style="background-image: url($m.getImage())">
        <h1>$m.getName()</h1>
        <p>$m.prettyPrice()</p>
    </div>
    </a>
    #end
</div>