<section class="index_header equipment-header">
<h1>Equipment</h1>
</section>

<div class="all_items">
    #foreach ($e in $equipment)
    <a href="/equipment/$e.getId()">
    <div class="single_item" style="background-image: url($e.getImage())">
        <h1>$e.getName()</h1>
        <p>$e.prettyPrice()</p>
    </div>
    </a>
    #end
</div>