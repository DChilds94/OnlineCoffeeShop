<section class="index_header equipment-header">
    <h1>Coffee</h1>
</section>
<div class="all_items">
    #foreach ($c in $coffee)
    <a href="/coffee/$c.getId()">
    <div class="single_item" style="background-image: url($c.getImage())">
        <h1>$c.getName()</h1>
        <p>$c.prettyPrice()</p>
    </div>
    </a>
    #end